{
  "name": "mezclas",
  "version": "1.0.0",
  "description": "programa para mezclas LG",
  "main": "dist/bundle.mjs",
  "type": "module",
  "scripts": {
    "start": "node _passenger.cjs",
    "dev": "node --watch src/app.mjs",
    "devC": "console-ninja node --watch src/app.mjs",
    "lint": "eslint . --ext .js,.mjs",
    "test": "jest",
     "test:watch": "jest --watch",
    "docs": "jsdoc -c jsdoc.json",
    "build": "webpack --config webpack.config.mjs --mode production",
    "buildD": "webpack --config webpack.config.mjs --mode development --stats-error-details"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": ["js"],
    "testMatch": ["**/__tests__/**/*.js", "**/?(*.)+(spec|test).js"],
    "verbose": true
  },
  "author": "Darcka",
  "license": "LG",
  "keywords": [
    "nodejs",
    "express",
    "mysql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/tu-repositorio.git"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "compression": "^1.8.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "16.4.5",
    "ejs": "^3.1.10",
    "env-var": "7.5.0",
    "esm": "3.2.25",
    "exceljs": "^4.4.0",
    "express": "4.19.2",
    "express-fileupload": "1.5.0",
    "express-rate-limit": "^7.5.0",
    "fs": "0.0.1-security",
    "helmet": "^8.1.0",
    "js": "^0.1.0",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "mysql2": "3.10.1",
    "nodemailer": "^6.9.16",
    "sequelize": "6.37.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "web-push": "^3.6.7",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.7",
    "@babel/preset-env": "7.24.7",
    "babel-loader": "9.1.3",
    "jest": "^29.7.0",
    "nodemon": "3.1.3",
    "standard": "17.1.0",
    "webpack": "5.92.1",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  },
  "eslintConfig": {
    "extends": "standard"
  }
}
