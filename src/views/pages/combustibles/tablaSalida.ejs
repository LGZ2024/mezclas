<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <!-- <%- include("../../templates/head-pwa") %> -->
        <%- include("../../templates/master/cabecera") %>

</head>

<body>
    <div class="container-scroller">
        <!-- navcabecera -->
        <%- include("../../templates/master/navCabecera") %>
            <div class="container-fluid page-body-wrapper">
                <!-- nav lateral -->
                <%- include("../../templates/master/navLateral") %>
                    <!-- panel principal -->
                    <div class="main-panel">
                        <div class="content-wrapper">
                            <div class="d-xl-flex justify-content-between align-items-start">
                                <h2 class="text-dark font-weight-bold mb-2">
                                    <!-- <%= titulo %> -->
                                </h2>
                            </div>
                            <div class="row ">
                                <div class="col-md-12">
                                    <div class="card">
                                        <div class="card-body">
                                            <h4 class="card-title">Salidas de Combustible</h4>
                                            <div class="row mb-5 justify-content-end">
                                                <div class="col-md-3">
                                                    <label for="min">Fecha desde:</label>
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-text input-group-text-sm">
                                                            <i class="mdi mdi-calendar-today"></i>
                                                        </span>
                                                        <input type="text" id="min" name="min"
                                                            class="form-control form-control-sm"
                                                            data-inputmask-alias="datetime"
                                                            data-inputmask-inputformat="yyyy-mm-dd"
                                                            placeholder="YYYY-MM-DD">
                                                    </div>
                                                </div>
                                                <div class="col-md-3">
                                                    <label for="max">Fecha hasta:</label>
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-text input-group-text-sm">
                                                            <i class="mdi mdi-calendar-today"></i>
                                                        </span>
                                                        <input type="text" id="max" name="max"
                                                            class="form-control form-control-sm"
                                                            data-inputmask-alias="datetime"
                                                            data-inputmask-inputformat="yyyy-mm-dd"
                                                            placeholder="YYYY-MM-DD">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="table-responsive w-100">
                                                <table id="tbSalidas" class="table table-striped table-bordered w-100">
                                                    <thead>
                                                        <tr>
                                                            <th>ID Salida</th>
                                                            <th>Area Logistica</th>
                                                            <th>Centro de coste</th>
                                                            <th>Fecha</th>
                                                            <th>Unidad</th>
                                                            <th>Combustible</th>
                                                            <th>Cantidad</th>
                                                            <th>Responsable</th>
                                                            <th>Temporada</th>
                                                            <th>Actividad</th>
                                                            <th>Comentarios</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>

                                                    </tbody>
                                                 
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- content-wrapper ends -->
                        <!-- partial:partials/_footer.html -->
                        <%- include("../../templates/master/footer") %>
                            <!-- partial -->
                    </div>
            </div>
            <!-- main-panel ends -->
    </div>
    <!-- container-scroller -->
    <%- include("../../templates/master/scripts") %>
        <script type="module" src="/app/activos/main.js"></script>
</body>

</html>