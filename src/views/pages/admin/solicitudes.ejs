<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">    
    <%- include("../../templates/head-pwa") %>
    <%- include("../../templates/admin/cabecera") %>

</head>

<body>
    <div class="container-scroller">
        <!-- navcabecera -->
        <%- include("../../templates/admin/navCabecera") %>
            <div class="container-fluid page-body-wrapper">
                <!-- nav lateral -->
                <%- include("../../templates/admin/navLateral") %>
                    <!-- panel principal -->
                    <div class="main-panel">
                        <div class="content-wrapper">
                            <div class="d-xl-flex justify-content-between align-items-start">
                                <h2 class="text-dark font-weight-bold mb-2">
                                    <!-- <%= titulo %> -->
                                </h2>
                            </div>
                            <div class="row ">
                                <div class="col-md-12">
                                    <div class="card">
                                        <div class="card-body">
                                            <h4 class="card-title">Solicitudes Completadas</h4>
                                            <div class="row mb-7 ">
                                                <div class="col-md-6 d-flex gap-2">
                                                    <button type="button" class="btn btn-inverse-secondary btn-fw"
                                                        id="descargarReporte">Descargar Reporte</button>
                                                    <button type="button" class="btn btn-inverse-primary btn-fw"
                                                        id="descargarReporteV2">Descargar Reporte completo</button>
                                                </div>
                                            </div>
                                            <div class="row mb-5 justify-content-end">
                                                <div class="col-md-3">
                                                    <label for="min">Fecha desde:</label>
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-text input-group-text-sm">
                                                            <i class="mdi mdi-calendar-today"></i>
                                                        </span>
                                                        <input type="text" id="min" name="min"
                                                            class="form-control form-control-sm"
                                                            data-inputmask-alias="datetime"
                                                            data-inputmask-inputformat="yyyy-mm-dd">
                                                    </div>
                                                </div>
                                                <div class="col-md-3">
                                                    <label for="max">Fecha hasta:</label>
                                                    <div class="input-group input-group-sm">
                                                        <span class="input-group-text input-group-text-sm">
                                                            <i class="mdi mdi-calendar-today"></i>
                                                        </span>
                                                        <input type="text" id="max" name="max"
                                                            class="form-control form-control-smel s"
                                                            data-inputmask-alias="datetime"
                                                            data-inputmask-inputformat="yyyy-mm-dd">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="table-responsive w-100">
                                                <table id="solicitudes"
                                                    class="table table-striped table-bordered w-100">
                                                    <thead>
                                                        <tr>
                                                            <th>Solicitud </th>
                                                            <th>Solicita</th>
                                                            <th>Empresa</th>
                                                            <th>Rancho</th>
                                                            <th>Temporada</th>
                                                            <th>Descripcion</th>
                                                            <th>Folio</th>
                                                            <th>Cento de Coste</th>
                                                            <th>Variedad</th>
                                                            <th>Fecha Solicitud</th>
                                                            <th>Fecha Cierre</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>

                                                    </tbody>
                                                    <tfoot>
                                                        <tr>
                                                            <th></th>
                                                            <th></th>
                                                            <th></th>
                                                            <th></th>
                                                            <th></th>
                                                            <th></th>
                                                            <th></th>
                                                            <th></th>
                                                            <th></th>
                                                            <th></th>
                                                        </tr>
                                                    </tfoot>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- content-wrapper ends -->
                        <!-- partial:partials/_footer.html -->
                        <%- include("../../templates/admin/footer") %>
                            <!-- partial -->
                    </div>
            </div>
            <!-- main-panel ends -->
    </div>
    <!-- Modal -->
    <%- include("../../templates/admin/modal") %>

        <!-- container-scroller -->
        <%- include("../../templates/admin/scripts") %>
            <script type="module" src="../../../app/solicitudes/funciones.js"></script>
</body>

</html>