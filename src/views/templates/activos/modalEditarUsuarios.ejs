      <div class="modal fade" id="miModalEditar" tabindex="-1" role="dialog" aria-labelledby="miModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-header">
              <h5 class="modal-title" id="miModalLabel" style="color: black;">Editar Empleado</h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Cerrar">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>
            <div class="modal-body">
              <form id="formEditarUsuarios">
                <div class="form-group">
                  <label for="empleado_id" class="form-label">Numero de Empleado:</label>
                  <input type="text" class="form-control" name="empleado_idE" id="empleado_idE" placeholder="Numero Empleado" readonly>
                </div>
                <div class="form-group">
                  <label for="nombre" class="form-label">Nombre:</label>
                  <input type="text" class="form-control" name="nombreE" id="nombreE" placeholder="Nombre de empledo">
                </div>
                <div class="form-group">
                  <label for="apellido_paterno" class="form-label">Apellido Paterno:</label>
                  <input type="text" class="form-control" name="apellido_paternoE" id="apellido_paternoE" placeholder="Apellido paterno">
                </div>
                <div class="form-group">
                  <label for="apellido_materno" class="form-label">Apellido Materno:</label>
                  <input type="text" class="form-control" name="apellido_maternoE" id="apellido_maternoE" placeholder="Apellido materno">
                </div> 
                <div class="form-group">
                  <label for="departamento" class="form-label">Departamento:</label>
                  <select name="departamento" id="departamentoE" class="form-control">
                    <option value="">Seleciona Departamento</option>
                    <% departamentos.forEach(function(departamentos) { %>
                      <option value="<%= departamentos %>">
                        <%= departamentos %>
                      </option>
                    <% }); %>
                  </select>
                </div>
              </form>
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
              <button type="submit" form="formEditarUsuarios" class="btn btn-primary" id="btnRegistrar">Editar</button>
            </div>
          </div>
        </div>
      </div>